    "next": "^13.1.6",
    "npm": "^8.19.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.31.3",
    "styled-components": "^5.3.6",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@types/node": "18.11.7",
    "@types/react": "18.0.24",
